<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
      <Appenders>
          <Console name="Console" target="SYSTEM_OUT" follow="true">
              <PatternLayout pattern="%m%n%ex{full}"/>
          </Console>
      </Appenders>
      <Loggers>
        <Root level="info">
          <AppenderRef ref="Console"/>
        </Root>
      </Loggers>
</Configuration>
